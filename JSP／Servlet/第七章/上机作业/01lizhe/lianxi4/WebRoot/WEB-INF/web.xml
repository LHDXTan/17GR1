<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <servlet-name>InitParmServlet</servlet-name>
    <servlet-class>cn.jbit.servlet.InitParmServlet</servlet-class>
    <init-param>
      <param-name>initParam</param-name>
      <param-value>欢迎小菜包</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitParmServlet</servlet-name>
    <url-pattern>/InitParmServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>InitParmServlet</welcome-file>
  </welcome-file-list>
</web-app>